extends ../layout

block content
  head
    include ../static_content/head
    script(src="/dist/js/demo.js")
    link(rel="stylesheet" href="/dist/css/bootstrap-tagsinput.css")
    script(src="/dist/js/bootstrap-tagsinput.js")

  body.hold-transition.skin-blue.sidebar-mini
    .wrapper
      include ../static_content/navbar
      include ../static_content/sidebar

      .content-wrapper(style="margin-top: 50px;")      
        section.content-header
          h1
            | Buttons
            small Control panel       
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-dashboard
                |  Home
            |         
            li
              a(href='#') UI
            |         
            li.active Buttons

        // Main content
        section.content
          #myModal.modal.fade(role='dialog', style="margin-top:150px;")
            .modal-dialog
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') Ã—
                  h4.modal-title Add some labels
                .modal-body
                  label(for='pwd') Labels: &nbsp;
                  input(id="tagsInput" type="text" value="" data-role="tagsinput" placeholder="Add tags here...")
                .modal-footer
                  button.btn.btn-primary(type='button', data-dismiss='modal', ng-click="devDashboardCtrl.saveUIComponent()") OK

          .row
            .col-md-12
              .box.box-primary
                .box-header
                  i.fa.fa-edit
                  |               
                  h3.box-title Buttons
                |             
                .box-body.pad.table-responsive
                  p
                    | Various types of buttons. Using the base class 
                    code .btn
                  |               
                  table.table.table-bordered.text-center
                    tr
                      th Normal
                      |                   
                      th
                        | Large 
                        code .btn-lg
                      |                   
                      th
                        | Small 
                        code .btn-sm
                      |                   
                      th
                        | X-Small 
                        code .btn-xs
                      |                   
                      th
                        | Flat 
                        code .btn-flat
                      |                   
                      th
                        | Disabled 
                        code .disabled
                    |                 
                    tr
                      td(id="btn-default")
                        .row(id="btn-default-btn")
                          .col-md-12
                            div.pull-right
                              a.btn.btn-social-icon.pull-right(data-toggle="dropdown" href="#" aria-expanded="false")
                                i.fa.fa-plus(style="font-size: 13px")
                              ul.dropdown-menu(ng-if="devDashboardCtrl.applications.length > 0" style="position: absolute; top: 33px; left: 10px")
                                li(style="cursor: pointer; pointer-events:none; opacity: 0.6;" role="presentation")
                                  a(role="menuitem") Add element to Favorites
                                li(style="cursor: pointer;" role="presentation" ng-repeat="app in devDashboardCtrl.applications")
                                  a(role="menuitem" data-toggle="modal" data-target="#myModal" ng-click="devDashboardCtrl.addUIComponent(app.repo_id,'button','btn-default');") {{ app.name }}
                                    i.fa.fa-check.pull-right(ng-if="app.components.includes('btn-default')" style="position: relative; top: 3px; color: #008000;")
                        button.btn.btn-block.btn-default(type='button') Default
                                         
